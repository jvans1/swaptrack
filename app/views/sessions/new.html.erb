<h1 id="notice"><%=notice%></h1>
<h1>Log in</h1>
<%= form_tag sessions_path do %>
<div class="field">
  <%=label_tag :email%>
  <%=text_field_tag :email%>
</div>
<div class="field">
  <%=label_tag :password%>
  <%=text_field_tag :password%>
</div>
<div class="action">
  <%=submit_tag "Log in" %>
</div>
<%end%>