<%= form_for request, {url: user_request_path(current_user, request)} do |f|%>
<%= f.submit 'Accept', :value => 'Accept', :name=>"status" %>
<%= f.submit "Deny", :value=>"Deny", :name=>"status"%>
  <% if request.updateable?%> 
    <%= f.submit "Edit", :value=>"Edit", :name=>"status"%>
  <%end%>
<%end%>